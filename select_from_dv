SELECT *
FROM hub1
INNER JOIN link12 ON link12.HUB1_HKEY = hub1.HUB1_HKEY
INNER JOIN hub2 ON hub2.HUB2_HKEY = link12.HUB2_HKEY
INNER JOIN link23 ON link23.HUB2_HKEY = hub2.HUB2_HKEY
INNER JOIN hub3 ON hub3.HUB3_HKEY = link23.HUB3_HKEY
INNER JOIN sat11 ON sat11.HUB1_HKEY = hub1.HUB1_HKEY 
	AND sat11.LOAD_END_DATETIME IS NULL
INNER JOIN sat31 ON sat31.HUB3_HKEY = hub3.HUB3_HKEY 
	AND sat31.LOAD_END_DATETIME IS NULL
